<mapper namespace="com.oms.dao.CodeMgmtDAO">

    <select id="selectCodeMgmt"  resultType="codeMgmtResDTO">
        <![CDATA[
            SELECT 
                   dl_expd_g_cd     AS dlExpdGCd,
                   dl_expd_prvs_cd  AS dlExpdPrvsCd,
                   dl_expd_prvs_nm  AS dlExpdPrvsNm,
                   sort_sn          AS sortSn,
                   use_yn           AS useYn,
                   pprr_eeno        AS pprrEeno,
                   fram_dtm         AS framDtm,
                   updr_eeno        AS updrEeno,
                   mdfy_dtm         AS mdfyDtm
              FROM tb_code_mgmt
             
        ]]>
    </select>

</mapper>

