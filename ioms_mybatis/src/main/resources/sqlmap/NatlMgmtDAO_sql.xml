<mapper namespace="com.oms.dao.NatlMgmtDAO">

    <select id="selectNatlMgmtList"  resultType="natlMgmtResDTO">
    <![CDATA[
            SELECT 
                   dl_expd_co_cd    AS dlExpdCoCd,
                   dl_expd_nat_cd   AS dlExpdNatCd,
                   dl_expd_regn_cd  AS dlExpdRegnCd,
                   nat_nm           AS natNm,
                   dytm_pln_nat_cd  AS dytmPlnNatCd,
                   prdn_mst_nat_cd  AS prdnMstNatCd,
                   pprr_eeno        AS pprrEeno,
                   fram_dtm         AS framDtm,
                   updr_eeno        AS updrEeno,
                   mdfy_dtm         AS mdfyDtm
              FROM tb_natl_mgmt
              ]]>
    </select>
    
    

