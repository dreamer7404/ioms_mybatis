<mapper namespace="hae.basic.board.dao.PersonDAO">

    <resultMap id="person" type="hae.basic.board.model.Person">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
    </resultMap>

    <select id="selectPersonList"  resultMap="person">
    <![CDATA[
        SELECT 
            ID
            , NAME
        FROM PERSON
    ]]>
    </select>
    
</mapper>